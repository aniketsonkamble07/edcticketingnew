{
	"builds":[
		{
			"src":"EDCproject/wsgi.py",
			"use":"@vercel/python"
		},
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
	],
	"routes":[
		{
			"src":"/(.*)",
			"dest":"EDCproject/wsgi.py"
		}
	]
}